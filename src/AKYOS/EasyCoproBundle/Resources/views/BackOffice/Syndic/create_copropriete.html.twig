{% extends '@AKYOSEasyCopro/BackOffice/Syndic/layout.html.twig' %}


{% block title %}
    Création COPROPRIETE
{% endblock %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('assets/backoffice/css/syndic_dashboard.css') }}">

{% endblock %}

{% block titre_page %}
    Création d'une copropriété
{% endblock %}

{% block breadcrumb %}
    <li><a href="{{ path('syndic_index') }}">Tableau de bord</a></li>
    <li class="active">Créer une copropriété</li>
{% endblock %}

{% block content %}

    <div class="row">
        <div class="col-lg-offset-2 col-lg-8 ">
            <div class="widget">
                <div class="widget-image widget-image-xs">
                    <img src="{{ asset('assets/backoffice/img/medias/coownerships/city03.jpg') }}">
                    <div class="widget-image-content">
                        <h3 class="widget-heading text-light"><strong></strong></h3>
                    </div>
                </div>
                <div class="widget-content">
                        {{ form_start(my_form) }}
                        <div class="row">

                            <div class="col-md-11">
                                {{ form_label(my_form.nom, 'Nom', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                                <div class="col-md-8 input-group">
                                    {{ form_widget(my_form.nom, { 'attr': {'class': 'form-control', 'placeholder':'Nom'}}) }}
                                </div>
                            </div>
                        </div>
                            <div class="row">
                            <div class="col-md-11">
                                {{ form_label(my_form.immatriculation, 'N° d\'immatriculation', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                                <div class="col-md-8 input-group">
                                   {{ form_widget(my_form.immatriculation, { 'attr': {'class': 'form-control', 'placeholder':'N° d\'immatriculation'}}) }}
                                </div>
                            </div>
                        </div>

                    <div class="row">
                        <div class="col-md-11">
                            {{ form_label(my_form.dateImmat, 'Date d\'immatricuation', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.dateImmat, { 'attr': {'class': 'form-control', 'placeholder':'Date d\'immatriculation'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.adressePrinc, 'Adresse postale', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.adressePrinc, { 'attr': {'class': 'form-control', 'placeholder':'Adresse postale'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                    <div class="col-sm-11">
                        {{ form_label(my_form.adresseSec, 'Complément d\'adresse', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                        <div class="col-md-8 input-group">
                                {{ form_widget(my_form.adresseSec, { 'attr': {'class': 'form-control', 'placeholder':'Complément d\'adresse'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.codePostal, 'Code postal', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.codePostal, { 'attr': {'class': 'form-control', 'placeholder':'Code postal'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.ville, 'Ville', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.ville, { 'attr': {'class': 'form-control', 'placeholder':'Ville'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                    <div class="col-sm-11">
                        {{ form_label(my_form.dateDebutGestion, 'Début de gestion', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                        <div class="col-md-8 input-group">
                                {{ form_widget(my_form.dateDebutGestion, { 'attr': {'class': 'form-control', 'placeholder':'Date de début de gestion'}}) }}
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.parcelleCadastrale, 'Parcelle cadastrale', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.parcelleCadastrale, { 'attr': {'class': 'form-control', 'placeholder':'Parcelle(s) cadastrale'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.nbBatiments, 'Nombre de bâtiments', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.nbBatiments, { 'attr': {'class': 'form-control', 'placeholder':'Nombre de bâtiments'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.periodeConstruction, 'Période de construction', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.periodeConstruction, { 'attr': {'class': 'form-control', 'placeholder':'Période de construction'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.typeChauffage, 'Type de chauffage', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.typeChauffage, { 'attr': {'class': 'form-control', 'placeholder':'Type de chauffage'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.nbAscenseurs, 'Nombre d\'ascenceurs', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.nbAscenseurs, { 'attr': {'class': 'form-control', 'placeholder':'Nombre d\'ascenseurs'}}) }}
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-11">
                            {{ form_label(my_form.dateReglement, 'Date d\'établissement du réglement intérieur', {'label_attr': {'class': 'col-md-4 control-label'}}) }}
                            <div class="col-md-8 input-group">
                                {{ form_widget(my_form.dateReglement, { 'attr': {'class': 'form-control', 'placeholder':'Date d\'établissement du règlement intérieur'}}) }}
                            </div>
                        </div>
                    </div>

                    <div class="row text-center">
                            <div class="col-sm-12 text-center">
                                <div class="form-group">
                                    {{ form_widget(my_form.submit, { 'attr': {'class': 'btn btn-info'}}) }}
                                </div>
                            </div>

                        {{ form_errors(my_form) }}
                        {{ form_end(my_form) }}
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}

{% block scripts %}
{% endblock %}
